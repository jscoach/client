{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/service-worker.js",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/sitemap.xml",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/package/:package_user/:package_name*?",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "*",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    }
  ]
}
